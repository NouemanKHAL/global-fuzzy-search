{
  "name": "global-fuzzy-search",
  "displayName": "Global Fuzzy Search",
  "description": "Fuzzy search for content across your workspace files",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.75.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:global-fuzzy-search.fuzzySearch"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "searchResultsContainers",
          "title": "Search Results",
          "icon": "/home/nouemankhal/git-clone/global-fuzzy-search/search.png"
        }
      ]
    },
    "views": {
      "searchResultsContainers": [
        {
          "id": "searchResults",
          "name": "Search Results"
        }
      ]
    },
    "commands": [
      {
        "command": "global-fuzzy-search.fuzzySearch",
        "title": "Fuzzy Search"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^8.0.1",
    "@types/mocha": "^10.0.1",
    "@types/node": "^16.18.11",
    "@types/vscode": "^1.75.0",
    "@typescript-eslint/eslint-plugin": "^5.49.0",
    "@typescript-eslint/parser": "^5.49.0",
    "@vscode/test-electron": "^2.2.2",
    "eslint": "^8.33.0",
    "glob": "^8.1.0",
    "mocha": "^10.1.0",
    "typescript": "^4.9.4"
  },
  "dependencies": {
    "fast-find-in-files": "^0.1.1",
    "fast-string-search": "^1.4.3"
  }
}
